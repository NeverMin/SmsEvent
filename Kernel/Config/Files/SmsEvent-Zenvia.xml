        <Group>SmsEvent</Group>
        <SubGroup>SmsEvent::Gateways</SubGroup>
        <Setting>
            <Hash>
                <Item Key="account"></Item>
                <Item Key="code"></Item>
                <Item Key="dispatch">send</Item>
                <Item Key="from"></Item>
